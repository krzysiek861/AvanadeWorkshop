{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "type": "string"
    }
  },
  "variables": {
  },
  "resources": [
    {
      "type": "Microsoft.Bing/accounts",
      "apiVersion": "2020-06-10",
      "name": "[parameters('name')]",
      "location": "global",
      "sku": {
        "name": "F1"
      },
      "kind": "Bing.Search.v7",
      "properties": {
        "statisticsEnabled": false
      }
    }
  ],
  "outputs": {
    "primaryKey": {
      "type": "string",
      "value": "[listKeys(parameters('name'), '2020-06-10').key1]"
    },
    "queryKey": {
      "type": "string",
      "value": "[listKeys(parameters('name'), '2020-06-10').key2]"
    }
  }
}